.preview p {
  list-style-type: arabic-indic;
  direction: RTL;
  text-align: right;
  font-family: "Amiri", serif;
  font-size: 18px;
}
.preview p.text-style {
  text-decoration: underline;
  text-align: center;
  font-size: 25px;
}
.preview .img-fluid {
  width: 90px;
}
.preview hr {
  border: 1px solid #000000;
}
.preview .icon {
  color: #367d7c;
  font-size: 20px;
  cursor: pointer;
}
@media print {
  .preview .icon {
    display: none;
  }
}

